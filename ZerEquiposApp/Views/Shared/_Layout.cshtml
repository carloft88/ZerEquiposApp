<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ZerEquiposApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ZerEquiposApp.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css" />



</head>
<body>
    <header>
        @*Nuevo*@
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
            <div class="container">
                <a class="navbar-brand" asp-area="Cliente" asp-controller="Home" asp-action="Index">Menus de Navegación</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor02" aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarColor02">
                    <ul class="navbar-nav me-auto">
                       
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Equipos</a>

                            <div class="dropdown-menu">

                                <a class="dropdown-item" asp-area="Admin"  asp-controller="Equipoes" asp-action="Index">Equipos</a>
                                <a class="dropdown-item" asp-area="Admin"  asp-controller="Memorias" asp-action="Index">Memorias</a>
                                <a class="dropdown-item" asp-area="Admin"  asp-controller="Procesador" asp-action="Index">Procesadores</a>
                                <a class="dropdown-item" asp-area="Admin"  asp-controller="Licenciamientos" asp-action="Index">Licenciamientos</a>
                                <a class="dropdown-item" asp-area="Admin"  asp-controller="Sistemas" asp-action="Index">Sistemas Operativos</a>
                                <a class="dropdown-item" asp-area="Admin"  asp-controller="Categorias" asp-action="Index">Categorias de Equipo</a>
                                <a class="dropdown-item" asp-area="Admin"  asp-controller="Canon" asp-action="Index">Canon</a>
                            </div>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Empleados</a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item" asp-area="Admin"  asp-controller="Empleados" asp-action="Index">Empleados</a>
                                <a class="dropdown-item" asp-area="Admin"  asp-controller="Empresas" asp-action="Index">Empresas</a>
                                <a class="dropdown-item" asp-area="Admin"  asp-controller="Cargos" asp-action="Index">Cargos</a>
                                

                            </div>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                    
                </div>
            </div>
        </nav>
               
        
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - Administracion de Equiipos Zer - <a asp-area="Admin"  asp-controller="Home" asp-action="Privacy"></a>
        </div>
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery.serializeJSON/2.9.0/jquery.serializejson.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>
    <script src="https://kit.fontawesome.com/0f48d8c00d.js" crossorigin="anonymous"></script>
    <script type="text/javascript" src="~/js/Empleado.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
